{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "@babel/plugin-proposal-export-namespace-from",
    "@babel/plugin-proposal-optional-chaining",
    "lodash",
    "ramda",
    [
      "module-resolver",
      {
        "root": [
          "./"
        ]
      }
    ],
    [
      "import",
      {
        "libraryName": "antd"
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "dynamic-import-node"
      ]
    }
  }
}
